{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "main": "functions/server.js",
  "engine": {
    "node": "^14"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "@runroom/purejs": "^3.1.0",
    "glob": "^7.1.6",
    "sass": "1.26.3",
    "twing": "^4.0.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "netlify-lambda": "^1.6.3",
    "nodemon": "^2.0.3",
    "serverless-http": "^2.3.0"
  },
  "scripts": {
    "clean:compiled": "rm -rf ui/components/**/*.min.js ui/components/**/*.css api/public/images",
    "sass:build": "sass ui/components:ui/components --no-source-map --style=compressed",
    "js:build": "webpack --config webpack.config.js",
    "build-assets": "yarn sass:build && yarn js:build",
    "netlify:build": "netlify-lambda build express",
    "start": "nodemon server.js",
    "server": "yarn netlify:build && yarn start"
  },
  "keywords": [
    "express.js"
  ],
  "resolutions": {
    "minimist": "1.2.5"
  }
}
